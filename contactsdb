import sqlite3

conn = sqlite3.connect('contacts.db')
c = conn.cursor()

def create_table():
    c.execute('CREATE TABLE IF NOT EXISTS names(name TEXT NOT NULL, last_name TEXT NOT NULL, day INTEGER NOT NULL, month TEXT NOT NULL, year INTEGER NOT NULL, tel INTEGER, cel INTEGER)')

def input_entry():
    name = input('Name: ')
    last_name = input('Last Name: ')
    day = input('Day: ')
    month = input('Month: ')
    year = input('Year:')
    tel = input('Telephone')
    cel = input('Cellphone')
    c.execute("INSERT INTO familia (name, last_name, day, month, year, tel, cel) VALUES (?, ?, ?, ?, ?, ?, ?)",
              (name, last_name, day, month, year, tel, cel))
    conn.commit()

create_table()
input_entry()

